			<Name>PollingManager_DefaultPeriod</Name>
			<Description>Default Period (Polling Manager)</Description>
			<Type>read</Type>
			<Information>
				<Subtext>Default polling period. Definied by the developer.</Subtext>
			</Information>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
			</Display>
			<Measurement>
				<Type options="time">number</Type>
			</Measurement>
		</Param>
		<Param id="1005" trending="false">
			<Name>PollingManager_PeriodType</Name>
			<Description>Period Type (Polling Manager)</Description>
			<Type>read</Type>
			<Information>
				<Subtext>Defines whether to use default or custom period time for polling.</Subtext>
			</Information>
			<Interprete>
				<RawType>numeric text</RawType>
				<Type>double</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
			</Display>
			<Measurement>
				<Type>discreet</Type>
				<Discreets>
					<Discreet>
						<Display>Default Period</Display>
						<Value>1</Value>
					</Discreet>
					<Discreet>
						<Display>Custom Period</Display>
						<Value>2</Value>
					</Discreet>
				</Discreets>
			</Measurement>
		</Param>
		<Param id="1055" setter="true">
			<Name>PollingManager_PeriodType</Name>
